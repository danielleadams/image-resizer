{
  "name": "image-resizer",
  "version": "1.0.0",
  "description": "Web app for resizing images for security testing",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "format:check": "prettier --check ./lib/*.js ./*.js",
    "format:diff": "prettier -l ./lib/*.js ./*.js",
    "format:fix": "prettier --write ./lib/*.js ./*.js",
    "deps:scan": "retire --path ./node_modules -v"
  },
  "author": "adamzdanielle@gmail.com",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "sharp": "^0.23.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "retire": "^2.0.3"
  }
}
